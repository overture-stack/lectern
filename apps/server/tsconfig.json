{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"module": "CommonJS",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedParameters": true,
		"esModuleInterop": true,
		"noEmit": true,
		"outDir": "dist",
		"baseUrl": "./src",
		"skipLibCheck": true
	},
	"include": ["./src/**/*.ts"]
}
